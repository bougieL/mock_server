[{
  "name": "使用示例",
  "url": "",
  "method": "get",
  "jsonp": "jsonpcallback",
  "res": {
    "message": "ok",
    "code": 0,
    "data": {
      "userName": "@cname()",
      "userId": "@id()",
      "birthDate": "@date('yyyy-MM-dd')",
      "avatar": "@image(100x100, #3399ff)",
      "sign": "@csentence()",
      "age|1-100": 100,
      "record|1-20": [{
        "name": "@pick('IPhone X', 'IPhone XS', 'Macbook Pro')",
        "method": "@pick('Check-in', 'Lucky Draw', 'Exchange', 'Official reward')",
        "time": "@datetime()"
      }]
    }
  },
  "reqField": [
    "{String} userId 用户id"
  ],
  "resField": [
    "{String} userName 用户名",
    "{String} userId 用户id",
    "{String} birthDate 用户生日",
    "{String} avatar 用户头像",
    "{Number} age 用户年龄",
    "{Array} record 兑换记录",
    "{String} record.name 商品名称",
    "{String} record.method 兑换方式",
    "{String} record.time 兑换时间"
  ]
}]
